generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model employees {
  employee_id    Int       @id @default(autoincrement())
  first_name     String
  last_name      String
  email          String
  phone_number   String?
  hire_date      DateTime
  country_id     Int?
  salary         Float?
  commission_pct Float?
  department_id  Int?
  password_employee String?
}
